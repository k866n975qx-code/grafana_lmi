    {
      "id": 10,
      "type": "timeseries",
      "title": "Months to Goal Trend (90d)",
      "description": "How quickly the goal date is approaching. Lower is better. Compares basic projection vs pace-adjusted estimate.",
      "gridPos": {
        "x": 0,
        "y": 10,
        "w": 24,
        "h": 4
      },
      "datasource": {
        "type": "frser-sqlite-datasource",
        "uid": "lmi_sqlite"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "queryType": "table",
          "queryText": "SELECT (p.as_of_date_local || 'T00:00:00Z') AS time, p.goal_months_to_goal AS \"Months to Goal\", (COALESCE((SELECT t.months_to_goal FROM daily_goal_tiers t WHERE t.as_of_date_local = p.as_of_date_local AND t.tier = p.goal_likely_tier LIMIT 1), p.goal_months_to_goal) - COALESCE(p.goal_pace_months_ahead_behind, 0)) AS \"Revised Months to Goal\" FROM daily_portfolio p WHERE p.as_of_date_local >= date('now','-90 days') ORDER BY p.as_of_date_local ASC;",
          "rawQueryText": "SELECT (p.as_of_date_local || 'T00:00:00Z') AS time, p.goal_months_to_goal AS \"Months to Goal\", (COALESCE((SELECT t.months_to_goal FROM daily_goal_tiers t WHERE t.as_of_date_local = p.as_of_date_local AND t.tier = p.goal_likely_tier LIMIT 1), p.goal_months_to_goal) - COALESCE(p.goal_pace_months_ahead_behind, 0)) AS \"Revised Months to Goal\" FROM daily_portfolio p WHERE p.as_of_date_local >= date('now','-90 days') ORDER BY p.as_of_date_local ASC;"
        }
      ],
      "transformations": [
        {
          "id": "convertFieldType",
          "options": {
            "conversions": [
              {
                "targetField": "time",
                "destinationType": "time"
              }
