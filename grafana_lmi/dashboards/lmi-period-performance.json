    {
      "id": 11,
      "type": "table",
      "title": "Income Metrics (Latest Completed Month)",
      "gridPos": {
        "x": 0,
        "y": 9,
        "w": 24,
        "h": 5
      },
      "datasource": {
        "type": "frser-sqlite-datasource",
        "uid": "lmi_sqlite"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "queryType": "table",
          "queryText": "WITH latest AS (SELECT * FROM period_summary WHERE period_type='MONTH' ORDER BY period_end_date DESC LIMIT 1) SELECT 'Monthly Income' AS metric, monthly_income_start AS start_val, monthly_income_end AS end_val, monthly_income_delta AS delta FROM latest UNION ALL SELECT 'Forward 12m Income', forward_12m_start, forward_12m_end, forward_12m_delta FROM latest UNION ALL SELECT 'Current Yield %', yield_start, yield_end, yield_delta FROM latest UNION ALL SELECT 'Yield on Cost %', yield_on_cost_start, yield_on_cost_end, yield_on_cost_delta FROM latest;",
          "rawQueryText": "WITH latest AS (SELECT * FROM period_summary WHERE period_type='MONTH' ORDER BY period_end_date DESC LIMIT 1) SELECT 'Monthly Income' AS metric, monthly_income_start AS start_val, monthly_income_end AS end_val, monthly_income_delta AS delta FROM latest UNION ALL SELECT 'Forward 12m Income', forward_12m_start, forward_12m_end, forward_12m_delta FROM latest UNION ALL SELECT 'Current Yield %', yield_start, yield_end, yield_delta FROM latest UNION ALL SELECT 'Yield on Cost %', yield_on_cost_start, yield_on_cost_end, yield_on_cost_delta FROM latest;"
        }
      ],
      "options": {
        "showHeader": true,
        "cellHeight": "md"
      },
      "fieldConfig": {
        "defaults": {
          "decimals": 2
        },
        "overrides": [
